{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=${PGHOST};Port=${PGPORT};Database=${PGDATABASE};Username=${PGUSER};Password=${POSTGRES_PASSWORD}"
  },
  "AppSettings": {
    "Token": "${JWT_SECRET_KEY}",
    "Issuer": "BMS-Backend",
    "Audience": "BMS-Backend-Users"
  }
}
